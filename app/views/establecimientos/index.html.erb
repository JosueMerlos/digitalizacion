<h1>Lista De Establecimientos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @establecimientos.each do |establecimiento| %>
      <tr>
        <td><%= establecimiento.nombre %></td>
        <td><%= link_to 'Ver', establecimiento %></td>
        <td><%= link_to 'Editar', edit_establecimiento_path(establecimiento) %></td>
        <td><%= link_to 'Eliminar', establecimiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Establecimiento', new_establecimiento_path %>
